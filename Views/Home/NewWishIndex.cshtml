@model InTheBag.Models.Wishes

@{
    ViewData["Title"] = "Enter Your Wishes";
}

<h2>Enter Your Wishes</h2>

<form asp-action="NewWishIndex" method="post">
    <div class="form-group">
        <label asp-for="Wish1"></label>
        <input asp-for="Wish1" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Wish2"></label>
        <input asp-for="Wish2" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Wish3"></label>
        <input asp-for="Wish3" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Submit Wishes</button>
</form>
